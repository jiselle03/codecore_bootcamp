<% @projects.each do |project| %>
<div>
    <h5><%= link_to(project.title, project_path(project)) %></h5>
    <p><%= project.description %></p>
    <small>Due Date: <%= project.due_date.strftime("%B %d, %Y")  %></small><br>
    <small>Posted <%= time_ago_in_words(project.created_at) %> ago</small>
</div>
<% end %>