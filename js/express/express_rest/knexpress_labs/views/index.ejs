<%- include('./partials/header') %>

<h4>Notes</h4> 

<table class="table table-striped table-bordered">
        <thead class="thead-dark">
          <tr>
            <th>ID</th>
            <th>Content</th>
          </tr>
        </thead>
        <tbody>
<% for (let note of notes) { %>
          <tr>
            <td><a href="/notes/<%= note.id %>"><%= note.id %></a></td>
            <td><%= note.content %></td>
          </tr>
      <% }%>
    </tbody>
</table>

<a class="nav-link" href="/notes/new"><button class="btn btn-primary btn-block float-right" type="submit">Add New Note</button></a>

<%- include('./partials/footer') %>
